{
  "name": "@astrsk/design-system",
  "version": "0.8.1",
  "description": "astrsk Design System - Component library with Storybook",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./button": {
      "types": "./dist/components/Button/index.d.ts",
      "import": "./dist/components/Button/index.js"
    },
    "./input": {
      "types": "./dist/components/Input/index.d.ts",
      "import": "./dist/components/Input/index.js"
    },
    "./label": {
      "types": "./dist/components/Label/index.d.ts",
      "import": "./dist/components/Label/index.js"
    },
    "./labeled-input": {
      "types": "./dist/components/LabeledInput/index.d.ts",
      "import": "./dist/components/LabeledInput/index.js"
    },
    "./textarea": {
      "types": "./dist/components/Textarea/index.d.ts",
      "import": "./dist/components/Textarea/index.js"
    },
    "./labeled-textarea": {
      "types": "./dist/components/LabeledTextarea/index.d.ts",
      "import": "./dist/components/LabeledTextarea/index.js"
    },
    "./select": {
      "types": "./dist/components/Select/index.d.ts",
      "import": "./dist/components/Select/index.js"
    },
    "./avatar": {
      "types": "./dist/components/Avatar/index.d.ts",
      "import": "./dist/components/Avatar/index.js"
    },
    "./skeleton": {
      "types": "./dist/components/Skeleton/index.d.ts",
      "import": "./dist/components/Skeleton/index.js"
    },
    "./character-card": {
      "types": "./dist/components/CharacterCard/index.d.ts",
      "import": "./dist/components/CharacterCard/index.js"
    },
    "./session-card": {
      "types": "./dist/components/SessionCard/index.d.ts",
      "import": "./dist/components/SessionCard/index.js"
    },
    "./tab-bar": {
      "types": "./dist/components/TabBar/index.d.ts",
      "import": "./dist/components/TabBar/index.js"
    },
    "./accordion": {
      "types": "./dist/components/Accordion/index.d.ts",
      "import": "./dist/components/Accordion/index.js"
    },
    "./utils": {
      "types": "./dist/lib/utils.d.ts",
      "import": "./dist/lib/utils.js"
    },
    "./styles": "./dist/index.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsup && tailwindcss -i src/index.css -o dist/index.css --minify",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "design-system",
    "component-library",
    "react",
    "storybook",
    "astrsk"
  ],
  "author": "astrsk team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/astrskai/astrsk.git",
    "directory": "packages/design-system"
  },
  "homepage": "https://astrskai.github.io/astrsk/design-system/",
  "bugs": {
    "url": "https://github.com/astrskai/astrsk/issues"
  },
  "packageManager": "pnpm@10.14.0",
  "peerDependencies": {
    "react": "^18.3.0 || ^19.0.0",
    "react-dom": "^18.3.0 || ^19.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@storybook/addon-a11y": "^10.1.2",
    "@storybook/addon-docs": "^10.1.2",
    "@storybook/addon-vitest": "^10.1.2",
    "@storybook/react-vite": "^10.1.2",
    "@tailwindcss/cli": "^4.1.14",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^24.8.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vueless/storybook-dark-mode": "^10.0.4",
    "lucide-react": "^0.503.0",
    "playwright": "^1.56.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^10.1.2",
    "tailwindcss": "^4.1.14",
    "tsup": "^8.3.5",
    "typescript": "^5.9.3",
    "vite": "^6.3.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.3.1"
  }
}
